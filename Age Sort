#include<iostream>
#include<sstream>
#include<vector>
#include<algorithm>
using namespace std;
int main ()
{
	int n;
	while(scanf("%d",&n)&&n!=0){
	vector<int>v(n);
	for(int i=0;i<n;i++)
	scanf("%d",&v[i]);
	sort(v.begin(),v.end());
	for(int i=0;i<n;i++){
	printf("%d",v[i]);
	if(i+1<n)printf(" ");
	}
	printf("\n");
	}
	return 0;
}
